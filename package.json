{
  "name": "h5p-boilerplate",
  "version": "1.0.0",
  "description": "Example H5P Content Type for getting started",
  "private": true,
  "scripts": {
    "lint": "jshint src/scripts/*.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack -p",
    "watch": "./node_modules/.bin/webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h5p/h5p-boilerplate.git"
  },
  "keywords": [
    "h5p",
    "library"
  ],
  "author": "Joubel",
  "license": "MIT",
  "jshintConfig": {
    "esversion": 6,
    "strict": true
  },
  "babel": {
    "plugins": [
      ["@babel/plugin-transform-classes", {
        "loose": true
      }]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/preset-env": "^7.7.7",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "jshint": "latest",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "resolve-url-loader": "^3.1.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
